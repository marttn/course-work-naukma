@model IEnumerable<coursework.Models.RequiredSkills>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MinReqYears)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MaxReqYears)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MinReqYears)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaxReqYears)
            </td>
            @Html.HiddenFor(modelItem => item.PositionId)
        </tr>
    }

</table>
